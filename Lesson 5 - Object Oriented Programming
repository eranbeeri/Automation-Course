// create 1 package with 2 classes 

--- class number 1

package classes;

public class Student {
	private String name;
	private String email;
	private int age;
	private boolean active;
	private double avgGrade;
	
	

	
	public Student(String name, String email, int age, boolean active, double avgGrade) {
		
		this.name = name;
		this.email = email;
		this.age = age;
		this.active = active;
		this.avgGrade = avgGrade;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public boolean isActive() {
		return active;
	}
	public void setActive(boolean active) {
		this.active = active;
	}
	public double getAvgGrade() {
		return avgGrade;
	}
	public void setAvgGrade(double avgGrade) {
		this.avgGrade = avgGrade;
	}

	
	public int birthYear() { 
		int year = 2020 - this.age;
		return year;
	}
	
	public String toString() {
		return "Student [name=" + name + ", email=" + email + ", age=" + age + ", active=" + active + ", avgGrade="
				+ avgGrade + "]";
	}
	
	
		
	}
	

